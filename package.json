    "http-errors": "~1.6.2",
    "mustache": "^4.0.1",
    "nanoid": "^4.0.2",
    "nodemon": "^2.0.22",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "three": "0.115.0",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/ws": "^7.2.3",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
